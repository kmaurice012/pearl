          <vs-td>\n                {{ tr.Email }}\n              </vs-td>\n              <vs-td style=\"display: flex; gap: 10px\">\n                <vs-button\n                  :active=\"active == 0\"\n                  @click=\"\n                    () => test(tr.id, tr.FirstName, tr.LastName, tr.Email)\n                  \"\n                >\n                  EDIT\n                </vs-button>\n\n                <vs-button\n                  danger\n                  border\n                  :active=\"active == 1\"\n                  @click=\"deleteGamer(tr.id)\"\n                  :id=\"tr.id\"\n                >\n                  Delete\n                </vs-button>\n              </vs-td>\n            </vs-tr>\n          </template>\n        </vs-table>\n        <Gamers />\n      </div>\n    </template>\n  </div>\n</template>\n\n\n<script>\nimport Gamers from \"../services/Users.js\";\nimport Create from \"../components/Create.vue\";\nimport axios from \"axios\";\nlet gamers = Gamers.Gamers;\nconsole.log(gamers, \"gameeers\");\n\nexport default {\n  name: \"gamers\",\n  components: {\n    Gamers,\n    Create,\n  },\n\n  data: () => ({\n    search: \"\",\n    gamers,\n  }),\n  methods: {\n    async deleteGamer(id) {\n      await axios\n        .delete(`http://localhost:3000/gamers/${id}`)\n        .then((response) => {\n          console.log(response);\n          window.location.reload();\n          window.location.reload();\n        });\n      //  axios({\n      //   method: \"delete\",\n      //   url: \"http://localhost:3000/gamers\",\n      //   data: {\n      //     id: id\n      //   },\n      // })\n    },\n  },\n};\n</script>\n\n<style scoped>\n.table {\n  width: 900px;\n  margin-left: 500px;\n  padding-top: 145px;\n}\n</style>"]}]}